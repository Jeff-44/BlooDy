@model BlooDyWeb.Models.TransfusionModule.Transfusion

@{
    ViewData["Title"] = "Details Transfusion";
}

<h1>Details Transfusion: <span class="display-0">@Model.Id</span></h1>

<div>
    @* <h4>ExamenMedical</h4> *@
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.DateTransfusion)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.DateTransfusion)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Quantite)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Quantite)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Beneficiaire.Code)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Beneficiaire.Code)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Composante.TypeComposante.Libelle)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Composante.TypeComposante.Libelle)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Medecin.Code)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Medecin.Code)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Statut)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Statut)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Notes)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Notes)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="ModifierTransfusion" class="btn btn-danger" asp-route-id="@Model?.Id">Modifier</a>
    <a asp-action="Index" class="btn btn-info">Transfusions</a>
</div>
