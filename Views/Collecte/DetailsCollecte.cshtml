@model Collecte



@{
    ViewData["Title"] = "Details Collecte";
}

<h1>Details Collecte: <span class="display-0">@Model.Id</span></h1>

<div>
    @* <h4>ExamenMedical</h4> *@
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Id)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Id)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.DateCollecte)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.DateCollecte)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Centre.Code)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Centre.Code)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Centre.NomCentre)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Centre.NomCentre)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Centre.TypeCentre)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Centre.TypeCentre)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Organisateur)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Organisateur)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="ModifierCollecte" class="btn btn-danger" asp-route-id="@Model?.Id">Modifier</a>
    <a asp-action="Index" class="btn btn-info">Collectes</a>
</div>
